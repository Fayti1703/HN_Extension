<ConditionalActions>
	<Instantly>
		<!-- Enable ETAS -->
		<RunFunction FunctionName="addFlags:CSEC_Member" FunctionValue="0"/>
		<!-- Add looping CAS files -->
		<AddConditionalActions Filepath="Actions/Loops/playerOfflineStorage.xml" />
	<!-- 	<AddConditionalActions Filepath="Actions/randomActions.xml" />-->
		<!-- Lock and load -->
		<SetLock DelayHost="true" Delay="20"     Module="terminal" IsLocked="true"  IsHidden="false"	/>
		<SetLock DelayHost="true" Delay="20.25"  Module="netmap"   IsLocked="true"  IsHidden="false"	/>
		<!-- Apparently you can click buttons through a ram lock. Bullshit. -->
		<SetLock DelayHost="true" Delay="20.5"   Module= "ram"     IsLocked="false" IsHidden="true" 	/>
		<SetLock DelayHost="true" Delay="20.75"  Module="display"  IsLocked="false" IsHidden="true" 	/>
		<LaunchHackScript Filepath="HackScripts/StartHack" Delay="15"
			TargetComp="playerComp" SourceComp="emptyRelay" DelayHost="true" />
		<!-- Unload and unlock -->
		<SetLock DelayHost="true" Delay="120"    Module="terminal" IsLocked="false" IsHidden="false"	/>
		<SetLock DelayHost="true" Delay="120.25" Module="netmap"   IsLocked="false" IsHidden="false"	/>
		<SetLock DelayHost="true" Delay="120.50" Module="ram"      IsLocked="false" IsHidden="false"	/>
		<SetLock DelayHost="true" Delay="121.75" Module="display"  IsLocked="false" IsHidden="false"	/>
	</Instantly>
</ConditionalActions>
